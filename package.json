{
	"name": "@bracketed/utilities",
	"description": "A collection of utilities used across Bracketed projects.",
	"packageManager": "yarn@4.9.2",
	"version": "1.0.0",
	"private": true,
	"publishConfig": {
		"access": "public"
	},
	"license": "Apache-2.0",
	"author": "@bracketed",
	"main": "lib/cjs/index.cjs",
	"module": "lib/esm/index.mjs",
	"types": "lib/cjs/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./lib/esm/index.d.mts",
				"default": "./lib/esm/index.mjs"
			},
			"require": {
				"types": "./lib/cjs/index.d.ts",
				"default": "./lib/cjs/index.cjs"
			}
		}
	},
	"files": [
		"lib"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/bracketed/templates-package"
	},
	"homepage": "https://bracketed.co.uk",
	"sideEffects": false,
	"bugs": {
		"url": "https://github.com/bracketed/templates-package/issues"
	},
	"keywords": [
		"bracketed",
		"typescript"
	],
	"scripts": {
		"lint": "yarn prettier --write .",
		"validate": "yarn prettier --check .",
		"build": "tsup",
		"update": "yarn upgrade-interactive"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.10",
		"@bracketed/tsup-configuration": "^1.0.5",
		"@bracketed/typescript-config": "^4.7.1",
		"@swc/core": "^1.15.7",
		"@types/node": "^25.0.3",
		"esbuild": "^0.27.2",
		"esbuild-fix-imports-plugin": "^1.0.23",
		"esbuild-plugin-copy": "^2.1.1",
		"esbuild-plugin-file-path-extensions": "^2.1.4",
		"esbuild-plugin-version-injector": "^1.2.1",
		"tslib": "^2.8.1",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3"
	},
	"engines": {
		"node": ">=18"
	},
	"dependencies": {
		"axios": "^1.13.2",
		"chalk": "^5.6.2",
		"discord.js": "^14.25.1",
		"zod": "^4.2.1"
	}
}
